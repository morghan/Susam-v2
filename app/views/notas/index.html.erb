<h1>Listing notas</h1>

<table>
  <tr>
    <th>Subject</th>
    <th>Student</th>
    <th>Examen 1</th>
    <th>Acumulado 1</th>
    <th>Examen 2</th>
    <th>Acumulado 2</th>
    <th>Examen 3</th>
    <th>Acumulado 3</th>
    <th>Examen 4</th>
    <th>Acumulado 4</th>
  </tr>

<% @notas.each do |nota| %>
  <tr>
    <td><%=h nota.subject_id %></td>
    <td><%=h nota.student_id %></td>
    <td><%=h nota.examen_1 %></td>
		<td><%=h nota.acumulado_1 %></td>
    <td><%=h nota.examen_2 %></td>
		<td><%=h nota.acumulado_2 %></td>
    <td><%=h nota.examen_3 %></td>
		<td><%=h nota.acumulado_3 %></td>
    <td><%=h nota.examen_4 %></td>
		<td><%=h nota.acumulado_4 %></td>
    <td><%= link_to 'Show', nota %></td>
    <td><%= link_to 'Edit', edit_nota_path(nota) %></td>
    <td><%= link_to 'Destroy', nota, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New nota', new_nota_path %>
